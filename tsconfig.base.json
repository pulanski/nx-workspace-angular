{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "strictNullChecks": true,
    "strict": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-workspace/api/core/feature": ["libs/api/core/feature/src/index.ts"],
      "@nx-workspace/api/feed/feature": ["libs/api/feed/feature/src/index.ts"],
      "@nx-workspace/shared/data-access/models": ["libs/shared/data-access/models/src/index.ts"],
      "@nx-workspace/shared/ui/material": ["libs/shared/ui/material/src/index.ts"],
      "@nx-workspace/shared/ui/nebular": ["libs/shared/ui/nebular/src/index.ts"],
      "@nx-workspace/web-linkedin-home-feature": ["libs/web/linkedin/home/feature/src/index.ts"],
      "@nx-workspace/web-linkedin-home-ui-start-post": ["libs/web/linkedin/home/ui/start-post/src/index.ts"],
      "@nx-workspace/web-linkedin-shell-feature": ["libs/web/linkedin/shell/feature/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
