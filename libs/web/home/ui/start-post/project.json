{
  "projectType": "library",
  "root": "libs/web/home/ui/start-post",
  "sourceRoot": "libs/web/home/ui/start-post/src",
  "prefix": "nx-workspace",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/web/home/ui/start-post"],
      "options": {
        "project": "libs/web/home/ui/start-post/ng-package.json",
        "tailwindConfig": "libs/web/home/ui/start-post/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/web/home/ui/start-post/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/web/home/ui/start-post/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/web/home/ui/start-post"],
      "options": {
        "jestConfig": "libs/web/home/ui/start-post/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/web/home/ui/start-post/src/**/*.ts", "libs/web/home/ui/start-post/src/**/*.html"]
      }
    }
  },
  "tags": []
