{
  "projectType": "library",
  "root": "libs/web/shell/ui/layout",
  "sourceRoot": "libs/web/shell/ui/layout/src",
  "prefix": "nx-workspace",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": [
        "dist/libs/web/shell/ui/layout"
      ],
      "options": {
        "project": "libs/web/shell/ui/layout/ng-package.json",
        "tailwindConfig": "libs/web/shell/ui/layout/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/web/shell/ui/layout/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/web/shell/ui/layout/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/web/shell/ui/layout"
      ],
      "options": {
        "jestConfig": "libs/web/shell/ui/layout/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/web/shell/ui/layout/src/**/*.ts",
          "libs/web/shell/ui/layout/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
